{
  "trainer": {
    "max_epochs": 20000,
    "limit_val_batches": 1,
    "accumulate_grad_batches": 1,
    "default_root_dir": "./logs"
  },
  "train": {
    "log_interval": 200,
    "eval_interval": 1000,
    "seed": 1234,
    "max_epochs": 20000,
